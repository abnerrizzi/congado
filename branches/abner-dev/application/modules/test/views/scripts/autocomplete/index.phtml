<html>

<script type="text/javascript" src="/congado/public/scripts/library/jquery/jquery.fcbkcomplete.js"></script>
<link rel="stylesheet" href="/congado/public/styles/jquery/fcbkcomplete.css" type="text/css" media="screen" title="Test Stylesheet" charset="utf-8" />

<script language="JavaScript">
        $(document).ready(function() 
        {        
          
          $("#select2").fcbkcomplete({
            json_url: "/congado/public/json/animal",
            cache: true,
            filter_case: true,
            filter_hide: true,
			firstselected: true,
            //onremove: "testme",
			//onselect: "testme",
            filter_selected: true,
			maxitems: 5,
            newel: true        
          });		 
        });
		
		function testme(item)
	  	{	if ($.browser.mozilla) 
			{
				console.log(item);
			}
			else
			{
				alert(item);
			}		  	
	  	}    
    </script>

    
    <select id="select2" name="select2"></select>
    </html>